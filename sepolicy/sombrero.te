#============= init ==============
#allow init default_android_service:service_manager add;
#allow init device:chr_file { ioctl open read write };
allow init property_socket:sock_file write;
allow init self:netlink_socket create;
allow init servicemanager:binder { call transfer };
allow init tmpfs:lnk_file create;
#allow init vendor_file:file execute_no_trans;

#============= kernel ==============
#allow kernel self:capability dac_override;
allow kernel system_data_file:lnk_file read;
allow kernel unlabeled:file { open read };

#============= mediacodec ==============
allow mediacodec proc:file { getattr ioctl open read };
allow mediacodec sysfs:file { open read };

#============= netd ==============
allow netd self:capability sys_module;

#============= priv_app ==============
