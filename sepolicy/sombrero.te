#allow audioserver sysfs:file { open read };
#allow audioserver system_data_file:lnk_file read;
#allow audioserver unlabeled:dir search;

#============= bluetooth ==============
allow bluetooth system_data_file:file open;
allow bluetooth unlabeled:dir search;

#============= bootanim ==============
#allow bootanim device:chr_file { getattr ioctl open read write };

#============= fsck ==============
#allow fsck block_device:blk_file { ioctl open read write };

#============= hal_wifi_default ==============
